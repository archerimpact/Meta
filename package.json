{
  "name": "ArcherMeta",
  "version": "1.0.0",
  "author": "Archer HRC Team",
  "description": "A desktop tool for Mac, Windows, and Linux made with Electron for viewing and exporting photo and video metadata online.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "install-app-deps",
    "rebuild": "electron-rebuild -f -w sqlite3"
  },
  "productName": "Archer Meta",
  "repository": "https://github.com/venkatvarsha/archer-hrc-metadata",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-builder": "^20.9.0",
    "electron-packager": "^11.1.0",
    "electron-rebuild": "^1.7.3"
  },
  "dependencies": {
    "async": "^2.6.1",
    "chart.js": "^2.7.2",
    "choices.js": "^3.0.4",
    "echarts": "^4.0.4",
    "exif": "^0.6.0",
    "exiftool-vendored": "^4.20.0",
    "material-design-icons": "^3.0.1",
    "mustache": "^2.3.0",
    "npm": "^6.0.0",
    "rimraf": "^2.6.2",
    "sqlite3": "^4.0.0"
  },
  "build": {
    "appId": "com.archer.meta",
    "mac": {
      "category": "public.app-category.photography"
    }
  }
}
